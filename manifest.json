{
  "manifest_version": 2,

  "name": "ChangeIt",
  "version": "1.0",
  "description": "Webpage Modifyer",

  "author": [ {
  "name": "KUSHA B K",
  "email": "kusha.b.k@gmail.com"
 } ],
 
 "browser_action": {
    "default_icon": "icon.png"
  },

  "permissions": [
    "tabs", "<all_urls>",
    "http://*/*",
    "https://*/*"
  ],

 "content_scripts": [{
    "js": ["content.js"],
    "matches": ["*://*/*"]
  }]

}
